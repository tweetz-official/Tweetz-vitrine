  <script>
    document.addEventListener('DOMContentLoaded', function() {
      const toggle = document.getElementById('paradiseToggle');
      const simpleView = document.getElementById('simpleView');
      const enferView = document.getElementById('enferView');
      const enferViewFr = document.querySelector('.enfer-desktop');
      const enferViewEn = document.querySelector('.enfer-desktop-en');
      const enferViewEs = document.querySelector('.enfer-desktop-es');
      const enferMobile = document.querySelector('.enfer-mobile');
      const enferMobileEn = document.querySelector('.enfer-mobile-en');
      const enferMobileEs = document.querySelector('.enfer-mobile-es');
      const simpleViewFr = document.querySelector('.simple-desktop');
      const simpleViewEn = document.querySelector('.simple-desktop-en');
      const simpleViewEs = document.querySelector('.simple-desktop-es');
      const simpleMobileFr = document.querySelector('.simple-mobile');
      const simpleMobileEn = document.querySelector('.simple-mobile-en');
      const simpleMobileEs = document.querySelector('.simple-mobile-es');
      const title = document.querySelector('.paradise__title');
      const description = document.querySelector('.paradise__description');
      const lang = document.getElementById('lang');
      const mediaQuery = window.matchMedia("(max-width: 500px)");

    // Récupérer l'état sauvegardé du toggle et de la langue
    let savedLang = localStorage.getItem("selectedLang") || document.documentElement.lang || "en";
    let savedToggleState = localStorage.getItem("toggleState") === "true";
    
      toggle.checked = savedToggleState;
      lang.value = savedLang;
      document.documentElement.lang = savedLang;
      enferView.style.display = "block";

      function updateSVG(lang) {
        document.querySelectorAll('.svg-lang').forEach(svg => svg.style.display = "none");
        let svgToShow = document.getElementById(`svg-${lang}`);
        if (svgToShow) svgToShow.style.display = "block";
      }

      function handleMediaQueryChange(event) {
        let currentLang = document.documentElement.lang || "en";
        
        if (event.matches) {

          if (currentLang === "en") {
            enferMobile.style.display = "none";
            enferMobileEn.style.display = "block";
          }
          if (currentLang === "es") {
            enferMobile.style.display = "none";
            enferMobileEs.style.display = "block";
          }

          if (toggle.checked) {

            simpleMobileFr.style.display = "block";

            if (currentLang === "en") {
              simpleMobileFr.style.display = "none";
              simpleMobileEn.style.display = "block";
            }

            if (currentLang === "es") {
              simpleMobileFr.style.display = "none";
              simpleMobileEs.style.display = "block";
            }
          }
        }
      }

      // Charger le fichier JSON
      function loadTranslations(lang) {
        fetch("../assets/js/translations.json")
      .then((response) => response.json())
      .then((translations) => {
        const data = translations[lang] || translations["en"]; // Si la langue n'existe pas => "en"

        function updateText() {

          if (toggle.checked) {
          simpleView.style.display = 'block';
          enferView.style.display = 'none';
          title.textContent = data.paradise_title;
          description.textContent = data.paradise_content;

          simpleViewFr.style.display = lang === "fr" ? "block" : "none";
          simpleViewEn.style.display = lang === "en" ? "block" : "none";
          simpleViewEs.style.display = lang === "es" ? "block" : "none";
          
        } else {
          simpleView.style.display = 'none';
          enferView.style.display = 'block';
          title.textContent = data.enfer_title;
          description.textContent = data.enfer_content;

          enferViewFr.style.display = lang === 'fr' ? "block" : "none";
          enferViewEn.style.display = lang === 'en' ? "block" : "none";
          enferViewEs.style.display = lang === 'es' ? "block" : "none";
        }
        handleMediaQueryChange(mediaQuery);
        }

        updateText();
        toggle.addEventListener('change', updateText);
      })
      .catch((error) => console.log('Erreur de chargement des traductions :', error));
      }

      mediaQuery.addEventListener("change", handleMediaQueryChange);
      handleMediaQueryChange(mediaQuery);

      // Détection de la langue initiale dans la balise <html>
      let currentLang = document.documentElement.lang || "en";
      lang.value = currentLang;
      loadTranslations(currentLang);

      // Changement de langue avec le select
      lang.addEventListener("change", function() {
        location.reload();
        let newLang = this.value;
        document.documentElement.lang = newLang;
        localStorage.setItem("selectedLang", newLang);
        updateSVG(newLang);
        loadTranslations(newLang);
      });
    });
    
    </script>

